<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <Button
        android:id="@+id/rest_testBtn"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="开始REST请求" />

    <Button
        android:id="@+id/rest_jsonTestBtn"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="开始JSON请求" />
    <Button
        android:id="@+id/rest_custonTestBtn"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="自定义转化器测试" />
    
    <Button
        android:id="@+id/rest_asyncTestBtn"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="async测试" />
    
    <Button
        android:id="@+id/rest_queneTestBtn"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="队列任务测试" />
    <Button
        android:id="@+id/rest_mineTestBtn"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="队列任务测试" />
	<Button
        android:id="@+id/rest_mine1TestBtn"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="队列任务测试1" />
    <TextView
        android:id="@+id/rest_infoView"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" />

</LinearLayout>